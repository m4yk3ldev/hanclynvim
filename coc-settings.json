{
  "html.filetypes": ["django", "htmldjango", "html"],
  "html.format.enable": true,
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSave": true,
  "git.addedSign.text": " ",
  "git.changedSign.text": " ",
  "git.removedSign.text": "﫧",
  "git.topRemovedSign.text": "﫧",
  "git.changeRemovedSign.text": "▎",
  "codeLens.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.refreshOnInsertMode": true,
  "Lua.diagnostics.globals": ["vim"],
  "sumneko-lua.enableNvimLuaDev": true,
  "cSpellExt.enableDictionaries": ["portuguese-brazilian", "spanish"],
  "cSpell.language": "en,pt,pt_BR,es",
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  "cSpell.enabled": true,
  "diagnostic.infoSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.errorSign": " ",
  "diagnostic.hintSign": " ",
  "tabnine.shortcut": "TabNine",
  "tabnine.priority": 1,
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.formatting.provider": "autopep8",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--disable=C0111",
    "--load-plugins",
    "pylint_django"
  ],
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "suggest.highPrioritySourceLimit": 10,
  "suggest.languageSourcePriority": 95,
  "suggest.lowPrioritySourceLimit": 90,
  "suggest.triggerCompletionWait": 30,
  "suggest.autoTrigger": "always",
  "suggest.localityBonus": true,
  "suggest.enablePreselect": true,
  "suggest.removeDuplicateItems": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.triggerAfterInsertEnter": false,
  "suggest.detailField": "abbr",
  "suggest.noselect": false,
  "suggest.minTriggerInputLength": 4,
  "coc.source.file.priority": 60,
  "coc.source.buffer.priority": 50,
  "coc.preferences.formatOnType": true,
  "snippets.ultisnips.enable": false,
  "dialog.floatBorderHighlight": "FloatBorder",
  "suggest.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "hover.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "signature.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "winblend": 15,
    "borderhighlight": "FloatBorder"
  },
  "Lua.telemetry.enable": true
}
